<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.changji.cloud.auth.mapper.AuthAccountMapper">

    <select id="queryByUserName" resultType="com.changji.cloud.auth.model.AuthAccount">
        select username, password ,create_ip,status,user_id from auth_account where username =#{username}
    </select>
</mapper>
